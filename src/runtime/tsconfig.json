{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "jsx": "preserve",
    "types": ["nuxt", "node"],
    "paths": {
      "#imports": ["../../.nuxt/imports"],
      "#app": ["node_modules/nuxt/dist/app"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}